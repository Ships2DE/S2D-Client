; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Ships2.DE"
config/description="https://ships2.de/"
run/main_scene="res://Scenes/Login.tscn"
boot_splash/image="res://UI/Textures/Logo/s2d_logo_trimmed.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.113725, 0.113725, 0.188235, 1 )
config/icon="res://UI/Textures/Logo/s2d_icon_512.png"

[display]

window/size/width=1920
window/size/height=1080
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/smoothing/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true
theme/custom="res://UI/GameUITheme.tres"

[mono]

project/assembly_name="S2D-Client"

[physics]

common/physics_fps=50
common/enable_pause_aware_picking=true
2d/default_gravity=0
2d/default_linear_damp=5.0
2d/default_angular_damp=5.0
common/physics_interpolation=true

[rendering]

quality/filters/use_fxaa=true
quality/depth/hdr.mobile=true
environment/default_environment="res://default_env.tres"
